    "parcel": "^2.0.0-beta.3.1",
    "postcss": "^8.3.5",
    "posthtml-doctype": "^1.1.1",
    "posthtml-modules": "^0.7.4",
    "posthtml-shorten": "^1.0.0",
    "tailwindcss": "^2.2.4"
